<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="301.0" prefWidth="576.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="AttendanceSheetsProject.MainWindowController">
   <Button layoutX="108.0" layoutY="49.0" mnemonicParsing="false" onAction="#showGroupWindow" text="Add/change group"/>
   <Button layoutX="108.0" layoutY="98.0" mnemonicParsing="false" onAction="#markAttendanceWindow"
           text="Mark attendance"/>
   <Button layoutX="108.0" layoutY="151.0" mnemonicParsing="false" onAction="#viewAttendanceWindow"
           text="View attendance"/>
   <Button layoutX="108.0" layoutY="206.0" mnemonicParsing="false" onAction="#attendanceGraphWindow"
           text="Attendance graph"/>
   <TextField fx:id="groupIdField" layoutX="313.0" layoutY="129.0" promptText="Group number"/>
   <Button layoutX="313.0" layoutY="206.0" mnemonicParsing="false" onAction="#printAttendancePDF"
           text="Print Group Attendance"/>
</AnchorPane>
